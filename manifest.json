{
  "manifest_version": 2,
  "name": "EDITED smart-proxy",
  "version": "0.0.1",
  "description": "Extension used for smart-proxy",
  "icons": {
    "16": "logo/logo_16.png",
    "24": "logo/logo_24.png",
    "32": "logo/logo_32.png",
    "48": "logo/logo_48.png",
    "128": "logo/logo_128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "*://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}